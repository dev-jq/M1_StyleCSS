<?xml version="1.0"?>
<config>
  <modules>
    <JQ_StyleCSS>
      <version>0.1.0</version>
    </JQ_StyleCSS>
  </modules>
  <global>
    <helpers>
      <stylecss>
        <class>JQ_StyleCSS_Helper</class>
      </stylecss>
    </helpers>
    <blocks>
        <page>
            <rewrite>
                <html_head>JQ_StyleCSS_Block_Html_Head</html_head>
            </rewrite>
        </page>
    </blocks>
  </global>
  <frontend>
    <translate>
      <modules>
          <JQ_StyleCSS>
              <files>
                  <alternative>JQ_StyleCSS.csv</alternative>
              </files>
          </JQ_StyleCSS>
      </modules>
    </translate>
  </frontend>
  <adminhtml>
    <translate>
        <modules>
            <JQ_StyleCSS>
                <files>
                    <alternative>JQ_StyleCSS.csv</alternative>
                </files>
            </JQ_StyleCSS>
        </modules>
    </translate>
   </adminhtml>
</config> 